{
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.js"
    },
    "./headless": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.js"
    }
  },
  "browserslist": [
    "> 0.2%",
    "last 2 versions",
    "not dead",
    "Firefox ESR",
    "Chrome 61",
    "Firefox 60",
    "ios_saf 9"
  ],
  "scripts": {
    "start": "parcel serve src/index.html --no-cache",
    "build": "parcel build src/index.html",
    "build:embed": "parcel build src/embed.ts",
    "build:help": "parcel build src/help.html --public-url .",
    "build:headless": "rollup -c",
    "lang:add": "node src/languages/generate.js add",
    "lang:sync": "node src/languages/generate.js sync",
    "lang:build": "node src/languages/generate.js build"
  },
  "dependencies": {
    "@parcel/transformer-glsl": "2.16.0",
    "@parcel/transformer-sass": "2.16.0",
    "ag-psd": "28.3.1",
    "buffer": "6.0.3",
    "js-beautify": "1.15.4",
    "json5": "2.2.3",
    "mdn-polyfills": "5.20.0",
    "parcel": "2.16.0",
    "polygon-clipping": "0.15.7",
    "transformation-matrix": "3.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.9",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.9.1",
    "rollup": "^4.52.5",
    "rollup-plugin-dts": "^6.2.3",
    "typescript": "^5.9.3"
  }
}
